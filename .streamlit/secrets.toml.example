# .streamlit/secrets.toml.example
# RENAME THIS FILE TO "secrets.toml" AND REPLACE THE VALUES BELOW.
# DO NOT COMMIT "secrets.toml" TO GIT.

# Google AI Studio API Key (for Gemini 1.5 Flash)
# Get yours at: https://aistudio.google.com/
GOOGLE_API_KEY = "YOUR_GOOGLE_API_KEY_HERE"

# Neon Postgres Connection String
# Format: postgresql://user:password@host/dbname?sslmode=require
NEON_DB_CONNECTION_STRING = "postgresql://user:password@host/dbname?sslmode=require"

# Admin Password (for database writes)
ADMIN_PASSWORD = "change_this_to_a_strong_password"
